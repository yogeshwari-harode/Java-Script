  b -= 5;
  c %= 4;
  d *= 2;
  document.write(a);
  document.write("<br>");
   document.write(b);
   document.write("<br>");
    document.write(c);
    document.write("<br>");
     document.write(d);
     document.write("<br>");
</script>
