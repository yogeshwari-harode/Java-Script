				{fname : "Rahul" , lname : "Singh"},
				{fname : "Ram" , lname : "Kumar"},
				{fname : "Dheeraj" , lname : "Patel"},
				]

				var ary2 = ary1.map(test2);
				document.write(ary2);

				function test2(x){
					return x.fname + " " + x.lname ;
				}

